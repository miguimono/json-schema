{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "schema-ng19": {
      "projectType": "library",
      "root": "projects/schema-ng19",
      "sourceRoot": "projects/schema-ng19/src",
      "prefix": "sh",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/schema-ng19/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/schema-ng19/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/schema-ng19/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/schema-ng19/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "schema-ng16": {
      "projectType": "library",
      "root": "projects/schema-ng16",
      "sourceRoot": "projects/schema-ng16/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/schema-ng16/ng-package.json",
            "tsConfig": "projects/schema-ng16/tsconfig.lib.json"
          }
        }
      }
    }
  }
}
